using System;

class Program
{
    static void Main(string[] args)
    {/*Используя вложенные циклы, вывести таблицы умножения от второй до шестой*/
       for ( int i = 0; i<10; i++)
        {
            for (int j = 1; j < 10; j++)
            {
                Console.Write((i + 1) * (j + 1)+ " ");
            }
            Console.WriteLine();
        }
       
    }
}
