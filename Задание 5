using System;

class Program
{
    static void Main(string[] args)
    {/*Используя вложенные циклы, вывести таблицы умножения от второй до шестой*/
       for ( int i = 1; i<7; i++)
        {
            Console.WriteLine($"Таблица умножения на {i+1}");
            for (int j = 0; j < 10; j++)
            {
                Console.Write((i + 1) * (j + 1)+ " ");
            }
            Console.WriteLine();
        }
       
    }
}
